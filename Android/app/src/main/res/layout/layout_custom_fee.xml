<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/tvNameFeeCustom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Họ và tên học sinh"
            android:textSize="@dimen/size20dp"
            android:layout_margin="@dimen/size10dp"
            android:textColor="@color/black"/>

        <TextView
            android:id="@+id/tvPriceFeeCustom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Số điện thoại"
            android:layout_below="@+id/tvNameFeeCustom"
            android:textSize="@dimen/size20dp"
            android:layout_marginLeft="@dimen/size10dp" />
    </RelativeLayout>


</RelativeLayout>